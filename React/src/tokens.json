{
    "colors": {
      "background": "#F7F7F7",
      "highlight": "#3498db",
      "cardContent": "#FFFFFF",
      "formFields": "#E5E5E5",
      "secondaryHighlight": "#FFC300",
      "textPrimary": "#333333",
      "textSecondary": "#666666",
      "accentColor": "#FF5733",
      "successColor": "#28A745",
      "warningColor": "#FFA500"
    },
    "typography": {
      "fontFamily": "Arial, sans-serif",
      "fontSize": "16px",
      "headingFontFamily": "Helvetica, sans-serif",
      "headingFontSize": "24px"
    },
    "spacing": {
      "small": "8px",
      "medium": "16px",
      "large": "24px"
    },
    "border": {
      "radius": "4px",
      "width": "1px",
      "color": "#ccc"
    },
    "shadow": {
      "box": "0px 2px 4px rgba(0, 0, 0, 0.1)"
    },
    "form": {
      "inputHeight": "40px",
      "inputPadding": "10px",
      "buttonHeight": "40px",
      "buttonPadding": "10px 20px"
    },
    "taskBoard": {
      "taskPadding": "16px",
      "taskBorderColor": "#ccc",
      "taskBorderWidth": "1px",
      "taskBorderRadius": "4px"
    }
  }
  